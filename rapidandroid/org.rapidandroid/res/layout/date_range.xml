<?xml version="1.0" encoding="utf-8"?>
<TableLayout android:id="@+id/TableLayout01"
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent" android:scrollbarStyle="outsideOverlay" android:scrollbars="vertical" android:layout_height="fill_parent">
	<TableRow android:id="@+id/TableRow01" android:layout_height="wrap_content"
		android:layout_width="fill_parent">
		<Button android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_weight="1" android:text="@string/btn_daterange_startdate" android:id="@+id/range_btn_startdate"></Button>
		
	
<Button android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_weight="1" android:text="@string/btn_daterange_enddate" android:id="@+id/range_btn_enddate"></Button></TableRow>
	

	<TableRow android:id="@+id/TableRow02" android:layout_height="wrap_content"
		android:layout_width="fill_parent"><TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/txv_startdate" android:textSize="16sp" android:layout_gravity="center_horizontal" android:layout_weight="1"></TextView>
		
		<TextView android:id="@+id/txv_enddate" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:textSize="16sp" android:layout_gravity="center_horizontal" android:layout_weight="1"></TextView>
	
</TableRow>
	
	<TextView android:id="@+id/TextView02" android:layout_width="wrap_content"
		android:layout_height="20px" android:text=" " ></TextView>
	
		
		



<TableRow android:id="@+id/TableRow04" android:layout_height="wrap_content" android:layout_width="fill_parent"><Button android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/range_btn_lastday" android:text="@string/range_btn_lastday" android:layout_weight="1">
	</Button><Button android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/range_btn_lastweek" android:text="@string/range_btn_lastweek" android:layout_weight="1"></Button></TableRow><TableRow android:id="@+id/TableRow03" android:layout_height="wrap_content" android:layout_width="fill_parent"><Button android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/range_btn_last_month" android:text="@string/range_btn_lastmonth" android:layout_weight="1"></Button><Button android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/range_btn_last_threemonth" android:text="@string/range_btn_lastthreemonth" android:layout_weight="1"></Button></TableRow>
<Button android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/range_btn_entire_range" android:text="@string/btn_entire_range"></Button>

</TableLayout>
