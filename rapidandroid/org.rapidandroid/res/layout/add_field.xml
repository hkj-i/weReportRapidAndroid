<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:id="@+id/widget74"
    android:orientation="vertical"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_height="wrap_content" android:layout_width="fill_parent">
<TableLayout android:id="@+id/TableLayout02"
    android:layout_height="wrap_content" android:layout_width="fill_parent">
    <TableRow android:id="@+id/TableRow02" android:layout_width="wrap_content"
        android:layout_height="wrap_content">
    <TextView android:layout_height="wrap_content" style="?android:attr/listSeparatorTextViewStyle"
        android:text="@string/lbl_fieldname" android:id="@+id/lbl_fieldname" android:layout_width="wrap_content">
    </TextView>
    
    
    
    </TableRow>
    <EditText android:id="@+id/etx_fieldname"
	android:layout_width="fill_parent" android:layout_height="wrap_content"
	android:textSize="14sp" android:singleLine="true"></EditText>

	<TableRow android:id="@+id/TableRow03" android:layout_width="wrap_content"
		android:layout_height="wrap_content">
		<TextView android:layout_height="wrap_content"
			style="?android:attr/listSeparatorTextViewStyle" android:text="@string/lbl_fieldprompt"
			android:id="@+id/lbl_fieldprompt" android:layout_width="wrap_content">
		</TextView>

	</TableRow>
        <EditText android:id="@+id/etx_fieldprompt"
        android:layout_width="fill_parent" android:layout_height="wrap_content"
        android:textSize="14sp" android:singleLine="true"></EditText>
        <TextView android:id="@+id/lbl_fieldtypechoose"
        android:layout_width="fill_parent" android:layout_height="wrap_content"
        android:text="@string/lbl_fieldtypechoose" style="?android:attr/listSeparatorTextViewStyle">
    </TextView>
    <Spinner android:id="@+id/cbx_fieldtype" android:layout_width="fill_parent"
        android:layout_height="wrap_content">
    </Spinner>
</TableLayout>


	
	
	
	

	




</LinearLayout>